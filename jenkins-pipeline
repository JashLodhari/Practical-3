#!/usr/bin/env groovy

pipeline {
  agent none
  stages {
    stage('compile') {
      steps {
        script {
          echo "Compiling the application....."
        }
      }
    }
    stage('build') {
      steps {
        script {
         echo "Building the application....." 
        }
      }
    }
    stage('Test') {
      steps {
        script {
         echo "Testing the application....." 
        }
      }
    }
    
    stage('Deploy') {
      steps {
        script {
         echo "Deploy the application....." 
        }
      }
    }
  }
}
